{
  "manifest_version": 2,
  "name": "Favorite Photos",
  "author": "Steve",
  "version": "1.0.1",
  "description": "Chrome extension demo project",
  "icons": [
    {
      "src": "favicon.ico",
      "sizes": "64x64 32x32 24x24 16x16",
      "type": "image/x-icon"
    }
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage"
  ]
}