{
  "manifest_version": 2,
  "name": "Favorite Photos",
  "author": "Steve",
  "version": "1.0.1",
  "description": "Chrome extension demo project",
  "icons": {
    "16": "16x16.png",
    "32": "32x32.png",
    "128": "128x128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "storage"
  ]
}